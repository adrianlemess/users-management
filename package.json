{
  "name": "react-ts-vite-template",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Fabien MARIE-LOUISE",
    "url": "https://github.com/fabien-ml"
  },
  "type": "module",
  "scripts": {
    "analyze:network": "vite-bundle-visualizer -t network",
    "analyze:sunburst": "vite-bundle-visualizer -t sunburst",
    "analyze:tree": "vite-bundle-visualizer --open",
    "build": "npm run typecheck && vite build",
    "commit": "commit",
    "dev": "vite",
    "format": "prettier . --write --ignore-path .gitignore && git update-index --again",
    "lint": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
    "lint:ci": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "prepare": "husky install",
    "prettier:ci": "prettier . --check --ignore-path .gitignore",
    "serve": "npm run build && vite preview --port 3000",
    "test": "jest --watch",
    "test:ci": "jest",
    "test:coverage": "jest --silent --watchAll=false --coverage",
    "test:e2e": "start-server-and-test serve http://127.0.0.1:3000/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test serve http://127.0.0.1:3000/ 'cypress run'",
    "typecheck": "tsc --noEmit",
    "vite:analyze": "vite build --mode analyze"
  },
  "lint-staged": {
    "*.{css,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --write"
    ],
    "package.json": "npx sort-package-json"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^11.0.8",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.2",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@commitlint/prompt-cli": "^19.0.3",
    "@cypress/vite-dev-server": "^5.0.7",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/testing-library__user-event": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "commitizen": "^4.2.4",
    "cypress": "^13.6.6",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "sort-package-json": "^2.8.0",
    "start-server-and-test": "^2.0.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-bundle-visualizer": "^1.0.1"
  }
}
